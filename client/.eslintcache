atus(orderEdit._id, orderEdit.orderStatus)\n    .then(({data}) => {\n      if(data.message){\n        toast({\n          title: \"Order Updated\",\n          description: data.message,\n          status: \"success\",\n          duration: 2000\n        })\n        setOrderEdit({openModal: false})\n        setReload(!reload)\n      }\n    })\n  }\n\n  return (\n    <AdminLayout sectionTitle=\"Order\">\n      <Box d=\"flex\" justifyContent=\"space-around\">\n        {orderStatus.map((btn, idx) => (\n          <Button\n            key={idx}\n            disabled={btn === orderStatusType}\n            onClick={() => setOrderStatusType(btn)}\n          >\n            {btn}\n          </Button>\n        ))}\n      </Box>\n      <SimpleGrid columns={4} spacing={10} mt=\"30px\">\n        {orders\n          .filter((orders) => orders.orderStatus === orderStatusType)\n          .map((order) => (\n            <Box\n              onClick={() => openOrder(order)}\n              key={order._id}\n              w=\"200px\"\n              borderWidth=\"1px\"\n              borderRadius=\"lg\"\n              cursor=\"pointer\"\n              p=\"10px\"\n            >\n              <Text as=\"p\">Order id: {order._id.slice(0, 12) + \"...\"}</Text>\n              <Text as=\"p\">Order Total: â‚¹{order.orderPrice}</Text>\n              <Text as=\"span\" color=\"gray.500\">\n                {moment(order.createdAt).fromNow()}\n              </Text>\n            </Box>\n          ))}\n      </SimpleGrid>\n      {orderEdit.openModal && (\n        <Modal\n          isOpen={orderEdit.openModal}\n          onClose={() => setOrderEdit({ ...orderEdit, openModal: false })}\n        >\n          <ModalOverlay />\n          <ModalContent>\n            <ModalHeader>Order View</ModalHeader>\n            <ModalCloseButton />\n            <ModalBody>\n              <Select\n                value={orderEdit.orderStatus}\n                onChange={(e) =>\n                  setOrderEdit({ ...orderEdit, orderStatus: e.target.value })\n                }\n              >\n                {orderStatus.map((status) => (\n                  <option value={status}>{status}</option>\n                ))}\n              </Select>\n              <Text as=\"p\" fontSize=\"15px\" m=\"15px\" textAlign=\"right\">\n                Total: {orderEdit.orderPrice}\n              </Text>\n              <Accordion allowMultiple>\n                <AccordionItem>\n                  <AccordionButton>Show Order Content<AccordionIcon /></AccordionButton>\n                  <AccordionPanel pb={4}>\n                    {orderEdit.orderContent.map((item) => {\n                      return <CartItem item={item} admin />;\n                    })}\n                  </AccordionPanel>\n                </AccordionItem>\n              </Accordion>\n            </ModalBody>\n            <ModalFooter>\n              <Button\n                colorScheme=\"blue\"\n                mr={3}\n                onClick={() => setOrderEdit({ ...orderEdit, openModal: false })}\n              >\n                Close\n              </Button>\n              <Button onClick={handleOrderUpdate}>Save</Button>\n            </ModalFooter>\n          </ModalContent>\n        </Modal>\n      )}\n    </AdminLayout>\n  );\n};\n\nexport default AdminOrder;\n","/home/charanvasu/projects/pizza/client/src/pages/admin-order/admin-order.helper.js",[],"/home/charanvasu/projects/pizza/client/src/components/admin-addon/addon.helper.js",[],"/home/charanvasu/projects/pizza/client/src/components/admin-category/category.helper.js",[],"/home/charanvasu/projects/pizza/client/src/components/admin-item/item.helper.js",[],"/home/charanvasu/projects/pizza/client/src/pages/home/home.helper.js",[],"/home/charanvasu/projects/pizza/client/src/components/admin-addon/AddOnForm.component.jsx",[],{"ruleId":"208","replacedBy":"209"},{"ruleId":"210","replacedBy":"211"},{"ruleId":"212","severity":1,"message":"213","line":1,"column":23,"nodeType":"214","mes